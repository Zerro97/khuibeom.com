    "typecheck": "tsc --noEmit --incremental false --skipLibCheck"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.1",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify-json/mdi": "^1.1.53",
    "@nuxt/content": "^2.12.0",
    "@nuxt/image": "^1.3.0",
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/web-vitals": "^0.2.6",
    "@unocss/nuxt": "^0.46.5",
    "@unocss/reset": "^0.46.5",
    "@unocss/transformer-directives": "^0.46.5",
    "commitizen": "^4.3.0",
    "cypress": "^12.17.1",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.30.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nuxt": "^3.10.2",
    "nuxt-headlessui": "^1.1.5",
    "nuxt-icon": "^0.6.8",
    "nuxt-seo-kit": "^1.3.9",
    "typescript": "^4.9.5",
    "unplugin-icons": "^0.16.5",
    "vitest": "^0.25.8"
